generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String

  @@map("user")
}

model Nota {
  id          Int    @id @default(autoincrement())
  nameCliente String
  modelo      String
  defeito     String
  valor       String
}

model Cliente {
  id       Int    @id @default(autoincrement())
  nome     String
  telefone String
  cpf      String
}
